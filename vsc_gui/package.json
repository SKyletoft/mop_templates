{
	"name": "md407-code",
	"displayName": "md407-code",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md407.set_port",
				"title": "Set port"
			},
			{
				"command": "md407.set_rate",
				"title": "Set baud rate"
			},
			{
				"command": "md407.run",
				"title": "Do"
			},
			{
				"command": "md407.new_basic",
				"title": "Create new project from the Basic template"
			},
			{
				"command": "md407.new_crt",
				"title": "Create new project from the CRT template"
			},
			{
				"command": "md407.new_project",
				"title": "Create new project from the Datatekniskt Projekt template"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "media/md407.svg",
					"id": "md407-config",
					"title": "MD407 Config"
				}
			]
		},
		"views": {
			"md407-config": [
				{
					"id": "md407-actions",
					"name": "Action"
				},
				{
					"id": "md407-ports",
					"name": "Port Config"
				},
				{
					"id": "md407-rates",
					"name": "Baud Rate Config"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "md407.run",
					"when": "view == md407-actions",
					"group": "inline"
				},
				{
					"command": "md407.set_port",
					"when": "view == md407-ports",
					"group": "inline"
				},
				{
					"command": "md407.set_rate",
					"when": "view == md407-rates",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
